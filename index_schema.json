{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "time": {
      "type": "number",
      "description": "上次更新时间戳"
    },
    "data": {
      "type": "object",
      "description": "日期与对应文件路径的映射",
      "additionalProperties": {
        "type": "string",
        "description": "日期对应的文件路径，键为 YYYY-MM-DD 格式的日期"
      }
    }
  },
  "required": ["time", "data"]
}